<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="761.0" prefWidth="623.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <VBox fx:id="scenepane" alignment="CENTER" prefHeight="716.0" prefWidth="588.0" spacing="20.0">
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </padding>
      <children>
         <Label fx:id="titlet" prefHeight="44.0" prefWidth="647.0" text="STACK IMPLEMENTATION" textFill="#b92121">
            <font>
               <Font name="Calibri Light Italic" size="51.0" />
            </font>
         </Label>
         <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="624.0" prefWidth="567.0">
            <children>
               <Button fx:id="popbt" layoutX="26.0" layoutY="126.0" mnemonicParsing="false" onAction="#delete" prefHeight="43.0" prefWidth="306.0" text="POP" textFill="#cd2020">
                  <font>
                     <Font name="Arial Bold Italic" size="16.0" />
                  </font>
               </Button>
               <Button fx:id="displaybt" layoutX="26.0" layoutY="199.0" mnemonicParsing="false" onAction="#display" prefHeight="44.0" prefWidth="306.0" text="Display" textFill="#d92e9c">
                  <font>
                     <Font name="Arial" size="16.0" />
                  </font>
               </Button>
               <Button fx:id="pushbt" layoutX="250.0" layoutY="70.0" mnemonicParsing="false" onAction="#Insert" prefHeight="32.0" prefWidth="78.0" text="PUSH" textFill="#16613e">
                  <font>
                     <Font name="Arial Bold Italic" size="17.0" />
                  </font>
               </Button>
               <TextField fx:id="pusht" layoutX="26.0" layoutY="69.0" prefHeight="32.0" prefWidth="195.0" />
               <Button fx:id="helpbt" layoutX="482.0" layoutY="552.0" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Help" textFill="#b92020">
                  <font>
                     <Font name="Arial Bold Italic" size="24.0" />
                  </font>
               </Button>
               <TextArea fx:id="show" layoutX="32.0" layoutY="256.0" prefHeight="354.0" prefWidth="306.0" />
               <TextField fx:id="sizet" layoutX="26.0" layoutY="13.0" prefHeight="32.0" prefWidth="195.0" />
               <Button fx:id="sizebt" layoutX="250.0" layoutY="14.0" mnemonicParsing="false" onAction="#size" prefHeight="31.0" prefWidth="78.0" text="SIZE" textFill="#16613e">
                  <font>
                     <Font name="Arial Bold Italic" size="17.0" />
                  </font>
               </Button>
               <Button fx:id="logoutbt" layoutX="386.0" layoutY="551.0" mnemonicParsing="false" onAction="#logout" prefHeight="43.0" prefWidth="85.0" text="Exit" textFill="#680c0c">
                  <font>
                     <Font name="Calibri Bold Italic" size="24.0" />
                  </font>
               </Button>
            </children>
         </AnchorPane>
         <Label fx:id="welcomeText1" />
      </children>
   </VBox>
</VBox>
